{
    "name": "yarooze/alchemilla-bundle",
    "type": "symfony-bundle",
    "description": "Graph visualisation symfony bundle",
    "keywords": ["php", "js", "symfony", "reusable bundle", "graph"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaroslav Bleicher",
            "email": "yarooze@yahoo.de"
        }
    ],
    "autoload": {
        "psr-4": { "Yarooze\\AlchemillaBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Yarooze\\AlchemillaBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/dependency-injection": "^7.0"
    },
    "require-dev": {
        "symfony/http-kernel": "^7.0",
        "phpunit/phpunit": "^11.1",
        "phpstan/phpstan": "^1.10",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "^3.9",
        "friendsofphp/php-cs-fixer": "^3.54"
    }
}
